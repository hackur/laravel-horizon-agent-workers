{
  "mcpServers": {
    "filesystem": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "/Volumes/JS-DEV/pcrcard"
      ]
    },
    "git": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-git",
        "/Volumes/JS-DEV/pcrcard"
      ]
    },
    "fetch": {
      "command": "npx",
      "args": [
        "-y",
        "@kazh/mcp-fetch"
      ]
    }
  },
    "playwright-mcp": {
      "command": "npx",
      "args": [
        "-y",
        "@playwright/mcp"
      ]
    }
}
